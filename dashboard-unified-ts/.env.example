# Environment Variables for Dashboard
# Copy this file to .env and fill in your values

# Airtable Base ID (optional - defaults to appXblSpAMBQskgzB)
VITE_AIRTABLE_BASE_ID=appXblSpAMBQskgzB

# Use backend API for local development (true/false)
# If true, uses https://ponce-patient-backend.vercel.app
# If false, uses Vercel API routes
VITE_USE_BACKEND_API=true
